{
  "name": "schema",
  "version": "1.0.0",
  "description": "",
  "config": {
    "genNodeTypes": "./generated/types-evchargingspec",
    "genNodeZod": "./generated/ts-zod",
    "jsonSchema": "./generated/schemas",
    "schemaDest":  "../node/src/schemas",
    "schemaDist":  "../node/dist/schemas",
    "schemaYAMLfiles": "common.yaml project.yaml station.yaml port.yaml operating-costs.yaml session.yaml outage.yaml",
    "schemaJSONfiles": "common.json project.json station.json port.json operating-costs.json session.json outage.json"
  },
  "scripts": {
    "build": "npx npm-run-all gen schema",
    "gen": "npx npm-run-all gen:project gen:station gen:port gen:costs gen:session gen:outage gen:uptime",
    "gen:project": "npx quicktype -s schema ./project.yaml --out ${npm_package_config_genNodeTypes}/project.ts --lang ts --just-types --prefer-unions",
    "gen:station": "npx quicktype -s schema ./station.yaml --out ${npm_package_config_genNodeTypes}/station.ts --just-types --lang ts --prefer-unions",
    "gen:port": "npx quicktype -s schema ./port.yaml --out ${npm_package_config_genNodeTypes}/port.ts --just-types --lang ts --prefer-unions",
    "gen:costs": "npx quicktype -s schema ./operating-costs.yaml --out ${npm_package_config_genNodeTypes}/operating-costs.ts --just-types --lang ts --prefer-unions",
    "gen:session": "npx quicktype -s schema ./session.yaml --out ${npm_package_config_genNodeTypes}/session.ts --just-types --lang ts --prefer-unions",
    "gen:outage": "npx quicktype -s schema ./outage.yaml --out ${npm_package_config_genNodeTypes}/outage.ts --just-types --lang ts --prefer-unions",
    "gen:uptime": "npx quicktype -s schema ./uptime.yaml --out ${npm_package_config_genNodeTypes}/uptime.ts --just-types --lang ts --prefer-unions",
    "zod": "npx npm-run-all zod:project zod:station zod:port zod:costs zod:session zod:outage zod:uptime",
    "zod:project": "npx quicktype -s schema ./project.yaml --out ${npm_package_config_genNodeZod}/project.ts --lang typescript-zod",
    "zod:station": "npx quicktype -s schema ./station.yaml --out ${npm_package_config_genNodeZod}/station.ts --lang typescript-zod",
    "zod:port": "npx quicktype -s schema ./port.yaml --out ${npm_package_config_genNodeZod}/port.ts --lang typescript-zod",
    "zod:costs": "npx quicktype -s schema ./operating-costs.yaml --out ${npm_package_config_genNodeZod}/operating-costs.ts --lang typescript-zod",
    "zod:session": "npx quicktype -s schema ./session.yaml --out ${npm_package_config_genNodeZod}/session.ts --lang typescript-zod",
    "zod:outage": "npx quicktype -s schema ./outage.yaml --out ${npm_package_config_genNodeZod}/outage.ts --lang typescript-zod",
    "zod:uptime": "npx quicktype -s schema ./uptime.yaml --out ${npm_package_config_genNodeZod}/uptime.ts --lang typescript-zod",
    "schema": "npx npm-run-all schema:copyYAML schema:common schema:project schema:station schema:port schema:costs schema:session schema:outage schema:uptime schema:copyJSON",
    "schema:copyYAML": "cp ${npm_package_config_schemaYAMLfiles} ${npm_package_config_schemaDest} && cp ${npm_package_config_schemaYAMLfiles} ${npm_package_config_schemaDist}",
    "schema:copyJSON": "cp ${npm_package_config_schemaJSONfiles} ${npm_package_config_schemaDest} && cp ${npm_package_config_schemaJSONfiles} ${npm_package_config_schemaDist}",
    "schema:common": "yq common.yaml -o json >./common.json",
    "schema:project": "yq project.yaml -o json >./project.json",
    "schema:station": "yq station.yaml -o json >./station.json",
    "schema:port": "yq port.yaml -o json >./port.json",
    "schema:costs": "yq operating-costs.yaml -o json >./operating-costs.json",
    "schema:session": "yq session.yaml -o json >./session.json",
    "schema:outage": "yq outage.yaml -o json >./outage.json",
    "schema:uptime": "yq uptime.yaml -o json >./uptime.json",
    "watch": "npx nodemon --watch ./operating-costs.yaml --watch ./operating-data.yaml --watch ./outage.yaml --watch ./project.yaml --watch ./station.yaml --watch ./uptime.yaml --exec 'npm run build'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "quicktype": "^23.0.115"
  }
}
